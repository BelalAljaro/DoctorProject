            <InnerShadow height="13.21" radius="5.1325" width="9.32" />
         </effect></TextField>
      <PasswordField fx:id="PasswordTF" layoutX="154.0" layoutY="238.0" prefHeight="36.0" prefWidth="421.0" promptText="Enter Your Password">
         <effect>
            <InnerShadow choke="0.03" height="9.32" radius="4.6475" width="11.27" />
         </effect></PasswordField>
      <Label alignment="CENTER" layoutX="40.0" layoutY="160.0" prefHeight="36.0" prefWidth="94.0" text="Email">
         <font>
            <Font name="System Bold" size="19.0" />
         </font></Label>
      <Label alignment="CENTER" layoutX="42.0" layoutY="238.0" prefHeight="36.0" prefWidth="94.0" text="Password">
         <font>
            <Font name="System Bold" size="19.0" />
         </font></Label>
   </children>
</AnchorPane>
